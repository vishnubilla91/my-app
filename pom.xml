			    <id>build-info</id>
			    <goals>
				    <goal>publish</goal>
			    </goals>
			    <configuration>
				    <publisher>
					    <contextUrl>http://18.216.87.169:8081/artifactory</contextUrl>
					    <username>admin</username>
					    <password>Raagavi@123</password>
					    <repoKey>libs-release-local</repoKey>
					    <snapshotRepoKey>libs-snapshot-local</snapshotRepoKey>
				    </publisher>	    
